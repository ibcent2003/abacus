@page "{TransactionId}"
@using Microsoft.AspNetCore.Authorization
@using Microsoft.Extensions.Localization
@using Wbc.Application.Common.Enums
@using Wbc.Application.Common.Helper
@using Wbc.WebUI.Services
@model Wbc.WebUI.Areas.DutyCalculator.Pages.PendingDutyStatusModel
@inject IStringLocalizer<PendingDutyStatusModel> Localizer
@inject CommonLocalizationService LocalizationService
@inject IAuthorizationService AuthorizationService

@section PageName
{
    @Localizer["PageName"]
}



@section BreadCrumbs
{
    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="/Dashboard">@LocalizationService.Get("DashBoardRes")</a></li>
            <li class="breadcrumb-item"><a href="@Url.Page("PendingDutyList",new {area="DutyCalculator"})">@LocalizationService.Get("PendingDutyListRes")</a></li>
            <li class="breadcrumb-item active" aria-current="page">@LocalizationService.Get("PendingDutyStatusRes")</li>
        </ol>
    </nav>
}

<div class="row">


    <div class="col-12">
        <div class="card">
            <div class="card-header">
                <div class="alert alert-info  alert-dismissible" role="alert">
                   
                    <div class="alert-message">
                      Our expert are currently working on your duty result. The current status of your duty is @Model.Command.Status.ToUpper(). A notification would be sent to you upon completion. Below your vehicle information 
                    </div>                   
                </div>
              
            </div>
            <div class="card-body">

                <div class="row">
                    <div class="col-sm-12">
                        <form>
                            <table class="table table-striped table-bordered table-hover table-checkable datatable dataTable" id="datatables-buttons" aria-describedby="datatables-buttons_info">

                                <tbody>
                                    <tr class="warning">
                                        <td>
                                            Vehicle Type

                                        </td>
                                        <td>
                                            @Model.Command.VehicleTypeName
                                        </td>
                                        <td>
                                            Year Of Manufacture
                                        </td>
                                        <td>
                                            @Model.Command.Year
                                        </td>
                                    </tr>


                                    <tr class="warning">
                                        <td>
                                            Vehicle Make
                                        </td>
                                        <td class="auto-style4">@Model.Command.MakeName</td>
                                        <td class="auto-style9">
                                            <span id="dnn_ctr12242_DutyCalculatorResult_Label15" class="auto-style8">Model</span>
                                        </td>
                                        <td><span id="dnn_ctr12242_DutyCalculatorResult_vmodel">@Model.Command.ModelName</span></td>
                                    </tr>



                                    <tr class="warning">
                                        <td>
                                            Special Features

                                        </td>
                                        <td>
                                            @if (Model.Command.SpecialFeatureName == null)
                                            {
                                                <span>NA</span>
                                            }
                                            else
                                            {
                                                @Model.Command.SpecialFeatureName
                                            }

                                        </td>
                                        <td>
                                            Engine Capacity
                                        </td>
                                        <td>

                                            @Model.Command.EngineCapacity


                                        </td>
                                    </tr>

                                    <tr class="warning">
                                        <td>
                                            Seating Capacity

                                        </td>
                                        <td>

                                            @if (Model.Command.SeatingCapacity == 0)
                                            {
                                                <span>NA</span>
                                            }
                                            else
                                            {
                                                @Model.Command.SeatingCapacity
                                            }
                                        </td>
                                        <td>
                                            Fuel Type
                                        </td>
                                        <td>
                                            @if (Model.Command.FuelType == null)
                                            {
                                                <span>NA</span>
                                            }
                                            else
                                            {
                                                @Model.Command.FuelType
                                            }

                                        </td>
                                    </tr>


                                    <tr class="warning">
                                        <td>
                                            Currency

                                        </td>
                                        <td>
                                            @Model.Command.CurrencyName

                                        </td>
                                        <td>
                                            Country
                                        </td>
                                        <td>
                                            @Model.Command.CountryId

                                        </td>
                                    </tr>

                                </tbody>

                            </table>
                        </form>

                    </div>
                </div>


            </div>
        </div>
    </div>
</div>


