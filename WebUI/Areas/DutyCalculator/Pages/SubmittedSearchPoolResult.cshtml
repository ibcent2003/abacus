@page "{TransactionId}"
@using Microsoft.AspNetCore.Authorization
@using Microsoft.Extensions.Localization
@using Wbc.Application.Common.Enums
@using Wbc.Application.Common.Helper
@using Wbc.WebUI.Services
@model Wbc.WebUI.Areas.DutyCalculator.Pages.SubmittedSearchPoolResultModel
@inject IStringLocalizer<SubmittedSearchPoolResultModel> Localizer
@inject CommonLocalizationService LocalizationService
@inject IAuthorizationService AuthorizationService
@{
    ViewData["Title"] = Localizer["PageTitle"];
    Layout = "_PlainLayout";

}
@section PageName
{
    @Localizer["PageName"]
}

@section ActivePage
{
    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="/Dashboard">@LocalizationService.Get("DashBoardRes")</a></li>
            <li class="breadcrumb-item active" aria-current="page">@LocalizationService.Get("CalculatedDutyResultRes")</li>
        </ol>
    </nav>
}




<div class="section small_pt pb_20">
    <div class="container">
        <div class="row">


            <div class="col-sm-12 col-md-12">

                <div class="heading_s1">
            <h5>@Localizer["Header"]</h5>
        </div>
                <div class="alert alert-danger alert-dismissible" role="alert">
                    <div class="alert-icon">
                        <i class="far fa-fw fa-bell"></i>
                    </div>
                    <div class="alert-message">
                        <strong>Ops!</strong> @Localizer["ErrorMessage"]
                    </div>

                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                </div>

                <div class="">
                    <table class="table table-striped table-bordered table-hover table-checkable datatable dataTable" id="datatables-buttons" aria-describedby="datatables-buttons_info">

                        <tbody>
                            <tr class="warning">
                                <td>
                                    Vehicle Type

                                </td>
                                <td>
                                    @Model.Command.VehicleTypeName
                                </td>
                                <td>
                                    Year Of Manufacture
                                </td>
                                <td>
                                    @Model.Command.Year
                                </td>
                            </tr>


                            <tr class="warning">
                                <td>
                                    Vehicle Make
                                </td>
                                <td class="auto-style4">@Model.Command.MakeName</td>
                                <td class="auto-style9">
                                    <span id="dnn_ctr12242_DutyCalculatorResult_Label15" class="auto-style8">Model</span>
                                </td>
                                <td><span id="dnn_ctr12242_DutyCalculatorResult_vmodel">@Model.Command.ModelName</span></td>
                            </tr>



                            <tr class="warning">
                                <td>
                                    Special Features

                                </td>
                                <td>
                                    @if (Model.Command.SpecialFeatureName == null)
                                    {
                                        <span>NA</span>
                                    }
                                    else
                                    {
                                        @Model.Command.SpecialFeatureName
                                    }

                                </td>
                                <td>
                                    Engine Capacity
                                </td>
                                <td>

                                    @Model.Command.EngineCapacity


                                </td>
                            </tr>

                            <tr class="warning">
                                <td>
                                    Seating Capacity

                                </td>
                                <td>

                                    @if (Model.Command.SeatingCapacity == 0)
                                    {
                                        <span>NA</span>
                                    }
                                    else
                                    {
                                        @Model.Command.SeatingCapacity
                                    }
                                </td>
                                <td>
                                    Fuel Type
                                </td>
                                <td>
                                    @if (Model.Command.FuelType == null)
                                    {
                                        <span>NA</span>
                                    }
                                    else
                                    {
                                        @Model.Command.FuelType
                                    }

                                </td>
                            </tr>


                            <tr class="warning">
                                <td>
                                    Currency

                                </td>
                                <td>
                                    @Model.Command.CurrencyName

                                </td>
                                <td>
                                    Country
                                </td>
                                <td>
                                    @if (Model.Command.CountryId == 1)
                                    {
                                        <p>Ghana</p>
                                    }
                                    else
                                    {
                                        @Model.Command.CountryId
                                    }


                                </td>
                            </tr>

                        </tbody>

                    </table>
                </div>
            </div>

        </div>

    </div>
</div>


