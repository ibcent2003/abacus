@page "{TransactionId}"
@using Microsoft.AspNetCore.Authorization
@using Wbc.Application.Common.Enums
@using Wbc.WebUI.Services
@model Wbc.WebUI.Areas.Admin.Pages.NotificationToUserModel
@inject IStringLocalizer<NotificationToUserModel> Localizer
@inject CommonLocalizationService LocalizationService
@inject IAuthorizationService AuthorizationService

@{
    ViewData["Title"] = Localizer["PageTitle"];

}

@section PageName
{
    @Localizer["PageName"]
}


@section BreadCrumbs
{
    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="/Dashboard">@LocalizationService.Get("DashBoardRes")</a></li>
            <li class="breadcrumb-item"><a href="@Url.Page("PendingDutyApprovalList",new {area="Admin"})">@LocalizationService.Get("PendingDutyApprovalListRes")</a></li>
            <li class="breadcrumb-item active" aria-current="page">@LocalizationService.Get("PendingDutyApprovalDetailRes")</li>
        </ol>
    </nav>
}




<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-header">
                <h5 class="card-title mb-0">Duty Information</h5>
            </div>
            <div class="card-body">
                <p class="mb-0">This page allows you to send notification to user.</p>
               
            </div>
        </div>
    </div>
    <div class="col-12 col-lg-12">
        <div class="tab">
            <ul class="nav nav-tabs" role="tablist">
                <li class="nav-item"><a class="nav-link active" href="#tab-1" data-toggle="tab" role="tab">Customer Info</a></li>
                <li class="nav-item"><a class="nav-link" href="#tab-2" data-toggle="tab" role="tab">Vehicle Info</a></li>
                <li class="nav-item"><a class="nav-link" href="#tab-3" data-toggle="tab" role="tab">Upload Calculated Duty</a></li>
                <li class="nav-item"><a class="nav-link" href="#tab-4" data-toggle="tab" role="tab">Send Calculated Duty</a></li>
            </ul>
            <div class="tab-content">
                <div class="tab-pane active" id="tab-1" role="tabpanel">
                    <h4 class="tab-title">Customer Information</h4>
                    <table class="table table-striped table-bordered table-hover table-checkable datatable dataTable" id="datatables-buttons" aria-describedby="datatables-buttons_info">

                        <tbody>
                            <tr class="warning">
                                <td>
                                    First Name:

                                </td>
                                <td>

                                    <span class="badge badge-success">John</span>
                                </td>
                                <td>
                                    Last Name:
                                </td>
                                <td>

                                    <span class="badge badge-success">Mike</span>
                                </td>
                            </tr>


                            <tr class="warning">
                                <td>
                                    Email Address:
                                </td>
                                <td>
                                    <span class="badge badge-success">john_mike@gmail.com</span>
                                </td>
                                <td>
                                    Mobile Number:
                                </td>
                                <td>
                                    <span class="badge badge-success">049339493</span>
                                </td>
                            </tr>





                        </tbody>

                    </table>

                    <hr />
                    <h4 class="tab-title">Subscription Package</h4>


                    <table class="table table-striped table-bordered table-hover table-checkable datatable dataTable" id="datatables-buttons" aria-describedby="datatables-buttons_info">

                        <tbody>
                            <tr class="warning">
                                <td>
                                    Subscription Type:

                                </td>
                                <td>

                                    <span class="badge badge-primary">Duty Calculator</span>
                                </td>
                                <td>
                                    Subscription Plan:
                                </td>
                                <td>

                                    <span class="badge badge-primary">Business Plan (30 Uses)</span>
                                </td>
                            </tr>


                            <tr class="warning">
                                <td>
                                    No of Use Left:
                                </td>
                                <td>
                                    <span class="badge badge-primary">25</span>
                                </td>
                                <td>
                                    Country:
                                </td>
                                <td>
                                    <span class="badge badge-primary">Ghana</span>
                                </td>
                            </tr>





                        </tbody>

                    </table>


                </div>
                <div class="tab-pane" id="tab-2" role="tabpanel">

                    <div class="row">


                        <div class="col-12">
                            <div class="card">
                                <div class="card-header">
                                    <div class="alert alert-info  alert-dismissible" role="alert">

                                        <div class="alert-message">
                                           The vehicle information is shown below
                                        </div>
                                    </div>

                                </div>
                                <div class="card-body">

                                    <div class="row">
                                        <div class="col-sm-12">
                                         <table class="table table-striped table-bordered table-hover table-checkable datatable dataTable" id="datatables-buttons" aria-describedby="datatables-buttons_info">

                                                <tbody>
                                                    <tr class="warning">
                                                        <td>
                                                            Vehicle Type

                                                        </td>
                                                        <td>
                                                            @Model.vehicleInfo.VehicleTypeName
                                                        </td>
                                                        <td>
                                                            Year Of Manufacture
                                                        </td>
                                                        <td>
                                                            @Model.vehicleInfo.Year
                                                        </td>
                                                    </tr>


                                                    <tr class="warning">
                                                        <td>
                                                            Vehicle Make
                                                        </td>
                                                        <td class="auto-style4">@Model.vehicleInfo.MakeName</td>
                                                        <td class="auto-style9">
                                                            <span id="dnn_ctr12242_DutyCalculatorResult_Label15" class="auto-style8">Model</span>
                                                        </td>
                                                        <td><span id="dnn_ctr12242_DutyCalculatorResult_vmodel">@Model.vehicleInfo.ModelName</span></td>
                                                    </tr>



                                                    <tr class="warning">
                                                        <td>
                                                            Special Features

                                                        </td>
                                                        <td>
                                                            @if (Model.vehicleInfo.SpecialFeatureName == null)
                                                            {
                                                                <span>NA</span>
                                                            }
                                                            else
                                                            {
                                                                @Model.vehicleInfo.SpecialFeatureName
                                                            }

                                                        </td>
                                                        <td>
                                                            Engine Capacity
                                                        </td>
                                                        <td>

                                                            @Model.vehicleInfo.EngineCapacity


                                                        </td>
                                                    </tr>

                                                    <tr class="warning">
                                                        <td>
                                                            Seating Capacity

                                                        </td>
                                                        <td>

                                                            @if (Model.vehicleInfo.SeatingCapacity == 0)
                                                            {
                                                                <span>NA</span>
                                                            }
                                                            else
                                                            {
                                                                @Model.vehicleInfo.SeatingCapacity
                                                            }
                                                        </td>
                                                        <td>
                                                            Fuel Type
                                                        </td>
                                                        <td>
                                                            @if (Model.vehicleInfo.FuelType == null)
                                                            {
                                                                <span>NA</span>
                                                            }
                                                            else
                                                            {
                                                                @Model.vehicleInfo.FuelType
                                                            }

                                                        </td>
                                                    </tr>


                                                    <tr class="warning">
                                                        <td>
                                                            Currency

                                                        </td>
                                                        <td>
                                                            @Model.vehicleInfo.CurrencyName

                                                        </td>
                                                        <td>
                                                            Importing Country
                                                        </td>
                                                        <td>
                                                            @if (Model.vehicleInfo.CountryId == 1)
                                                            {
                                                                <span class="badge badge-primary">Ghana</span>
                                                            }
                                                            else
                                                            {
                                                                <span class="badge badge-primary">@Model.vehicleInfo.Country.CountryName</span>
                                                            }

                                                        </td>
                                                    </tr>

                                                </tbody>

                                            </table>
                                        </div>
                                    </div>


                                </div>
                            </div>
                        </div>
                    </div>

                </div>


                <div class="tab-pane" id="tab-3" role="tabpanel">
                    <div class="col-md-12">
                        <div class="card">

                            <div class="card-body">
                            
                                    <input type="hidden" asp-for="vehicleInfo.TransactionId" />


                                    <div class="form-row">
                                        <div class="form-group col-md-4">
                                            <label class="form-label">Vehicle Type</label>
                                            <input class="form-control" asp-for="vehicleInfo.VehicleTypeName" readonly="readonly">
                                        </div>
                                        <div class="form-group col-md-4">
                                            <label class="form-label">Make</label>
                                            <input class="form-control" asp-for="vehicleInfo.MakeName" readonly="readonly" />
                                        </div>
                                        <div class="form-group col-md-4">
                                            <label class="form-label">Model</label>
                                            <input class="form-control" asp-for="vehicleInfo.ModelName" readonly="readonly">
                                        </div>
                                    </div>

                                    <div class="form-row">
                                        <div class="form-group col-md-4">
                                            <label class="form-label">Year of Manufacture</label>
                                            <input class="form-control" asp-for="vehicleInfo.Year" readonly="readonly">
                                        </div>
                                        <div class="form-group col-md-4">
                                            <label class="form-label">Speacial Features</label>
                                            <input class="form-control" asp-for="vehicleInfo.SpecialFeatureName" readonly="readonly" />
                                        </div>
                                        <div class="form-group col-md-4">
                                            <label class="form-label">Seating Capacity</label>
                                            <input class="form-control" asp-for="vehicleInfo.SeatingCapacity" readonly="readonly">
                                        </div>
                                    </div>

                                    <div class="form-row">

                                        <div class="form-group col-md-4">
                                            <label class="form-label">Fuel Type</label>
                                            <input class="form-control" asp-for="vehicleInfo.FuelType" readonly="readonly" name="validation-fuel" />
                                        </div>
                                        <div class="form-group col-md-4">
                                            <label class="form-label">Engine Capacity</label>
                                            <input class="form-control" asp-for="vehicleInfo.EngineCapacity" readonly="readonly" name="validation-cc" placeholder="">
                                        </div>
                                        <div class="form-group col-md-4">
                                            <label class="form-label">Currency</label>
                                            <input class="form-control" asp-for="vehicleInfo.CurrencyName" readonly="readonly" name="validation-currency" />
                                        </div>

                                    </div>

                                    <div class="form-row">
                                        <div class="form-group col-md-4">
                                            <label class="form-label">Home Delivery Value (HDV) </label>
                                            <input class="form-control" asp-for="vehicleInfo.HDV" readonly="readonly">
                                        </div>

                                        <div class="form-group col-md-4">
                                            <label class="form-label">No Of Doors</label>
                                            <input class="form-control" asp-for="CalCommand.NoOfDoors" readonly="readonly">
                                        </div>


                                        <div class="form-group col-md-4">
                                            <label class="form-label">Chasis No/VIN</label>
                                            <input class="form-control" asp-for="CalCommand.ChassisNo" readonly="readonly">
                                        </div>

                                    </div>

                                    <div class="form-row">
                                        <div class="form-group col-md-4">
                                            <label class="form-label">Depreciation Allow </label>
                                            <input class="form-control" asp-for="CalCommand.DepreciationAllow" readonly="readonly">
                                        </div>

                                        <div class="form-group col-md-4">
                                            <label class="form-label">FOB (Cost)</label>
                                            <input class="form-control" asp-for="CalCommand.FOB" readonly="readonly">
                                        </div>


                                        <div class="form-group col-md-4">
                                            <label class="form-label">Freight</label>
                                            <input class="form-control" asp-for="CalCommand.Freight" readonly="readonly">
                                        </div>

                                    </div>

                                    <div class="form-row">
                                        <div class="form-group col-md-4">
                                            <label class="form-label">Insurance </label>
                                            <input class="form-control" asp-for="CalCommand.Insurance" readonly="readonly">
                                        </div>
                                        <div class="form-group col-md-4">
                                            <label class="form-label">Exchange Rate</label>
                                            <input class="form-control" asp-for="CalCommand.ExchangeRate" readonly="readonly">
                                        </div>
                                        <div class="form-group col-md-4">
                                            <label class="form-label">CIF in GHCedis</label>
                                            <input class="form-control" asp-for="CalCommand.CIFLocalCurrency" readonly="readonly">
                                        </div>
                                    </div>

                                    <div class="form-row">
                                        <div class="form-group col-md-4">
                                            <label class="form-label">Import Duty </label>
                                            <input class="form-control" asp-for="CalCommand.ImportDuty" readonly="readonly">
                                        </div>
                                        <div class="form-group col-md-4">
                                            <label class="form-label">Processing Fees</label>
                                            <input class="form-control" asp-for="CalCommand.ProcessingFee" readonly="readonly">
                                        </div>
                                        <div class="form-group col-md-4">
                                            <label class="form-label">Vat</label>
                                            <input class="form-control" asp-for="CalCommand.VAT" readonly="readonly">
                                        </div>
                                    </div>

                                    <div class="form-row">
                                        <div class="form-group col-md-4">
                                            <label class="form-label">Shippers Fee </label>
                                            <input class="form-control" asp-for="CalCommand.Shipper" value="5.00" readonly="readonly">
                                        </div>
                                        <div class="form-group col-md-4">
                                            <label class="form-label">NHIL</label>
                                            <input class="form-control" asp-for="CalCommand.NHIL" readonly="readonly">
                                        </div>
                                        <div class="form-group col-md-4">
                                            <label class="form-label">Interest Charge</label>
                                            <input class="form-control" asp-for="CalCommand.InterestCharge" readonly="readonly">
                                        </div>
                                    </div>
                                    <div class="form-row">
                                        <div class="form-group col-md-4">
                                            <label class="form-label">Ecowas</label>
                                            <input class="form-control" asp-for="CalCommand.Ecowas" readonly="readonly">
                                        </div>
                                        <div class="form-group col-md-4">
                                            <label class="form-label">Network Charge</label>
                                            <input class="form-control" asp-for="CalCommand.NetworkCharge" readonly="readonly">
                                        </div>
                                        <div class="form-group col-md-4">
                                            <label class="form-label">EXIM</label>
                                            <input class="form-control" asp-for="CalCommand.EXIM" readonly="readonly">
                                        </div>
                                    </div>

                                    <div class="form-row">
                                        <div class="form-group col-md-4">
                                            <label class="form-label">NetCharge VAT</label>
                                            <input class="form-control" asp-for="CalCommand.NetChargeVAT" readonly="readonly">
                                        </div>
                                        <div class="form-group col-md-4">
                                            <label class="form-label">Examination Fee</label>
                                            <input class="form-control" asp-for="CalCommand.ExamFee" readonly="readonly">
                                        </div>
                                        <div class="form-group col-md-4">
                                            <label class="form-label">NetCharge NHIL</label>
                                            <input class="form-control" asp-for="CalCommand.NetChargeNHIL" readonly="readonly">
                                        </div>
                                    </div>
                                    <div class="form-row">
                                        <div class="form-group col-md-4">
                                            <label class="form-label">Special Import Levy</label>
                                            <input class="form-control" asp-for="CalCommand.SpecialImpLevy" readonly="readonly">
                                        </div>
                                        <div class="form-group col-md-4">
                                            <label class="form-label">OverAge Penalty</label>
                                            <input class="form-control" asp-for="CalCommand.OverAgePenalty" readonly="readonly">
                                        </div>
                                        <div class="form-group col-md-4">
                                            <label class="form-label">HSCode</label>
                                            <input class="form-control" asp-for="CalCommand.HsCode" readonly="readonly">
                                        </div>
                                    </div>

                                    <div class="form-row">
                                        <div class="form-group col-md-8">
                                            <label class="form-label">TOTAL ESTIMATED DUTY PAYABLE IN LOCAL CURRENCY BEFORE 30% DEDUCTION</label>
                                        </div>
                                        <div class="form-group col-md-4">
                                            <input class="form-control" asp-for="CalCommand.TotalDutyBeforeDeduction" readonly="readonly">
                                        </div>
                                    </div>

                                    <div class="form-row">
                                        <div class="form-group col-md-8">
                                            <label class="form-label">TOTAL ESTIMATED DUTY PAYABLE IN LOCAL CURRENCY AFTER 30% DEDUCTION</label>
                                        </div>
                                        <div class="form-group col-md-4">
                                            <input class="form-control" asp-for="CalCommand.TotalDutyAfterDeduction" readonly="readonly">
                                        </div>
                                    </div>

                                   

                             
                            </div>
                        </div>
                    </div>
                </div>

                <div class="tab-pane" id="tab-4" role="tabpanel">

                    <form asp-page="NotificationToUser" method="post">
                        <input type="hidden" asp-for="WithMessageCommand.TransactionId" />
                        <div class="form-group">

                            <textarea class="form-control is-invalid" asp-for="WithMessageCommand.DutyResult" aria-invalid="true"></textarea>
                        </div>

                        <div class="form-row">
                            <div class="form-group col-md-6">

                            </div>
                            <div class="form-group col-md-4">

                            </div>
                            <div class="form-group col-md-2">

                                <partial name="_FormSubmissionButtonPartial" model="FormSubmissionAction.Create" />

                            </div>
                        </div>


                    </form>
                </div>
            </div>
        </div>
    </div>
</div>


<script src="~/tinymce/tinymce.min.js"></script>

@section Scripts{

    <partial name="_ValidationScriptsPartial" />
    <script>
        tinymce.init({
            selector: "textarea",
            mode: "exact",
            elements: "newsform_NewsContent",
            theme: "modern",


            plugins: [
                "advlist autolink lists link image charmap print preview hr anchor pagebreak",
                "searchreplace wordcount visualblocks visualchars code fullscreen",
                "insertdatetime media nonbreaking save table contextmenu directionality",
                "emoticons template paste textcolor "
            ],
            toolbar1: "insertfile undo redo | styleselect | bold italic | alignleft aligncenter alignright alignjustify | bullist numlist outdent indent | link image",
            toolbar2: "print preview media | forecolor backcolor emoticons",
            image_advtab: true,
            templates: [
                { title: 'Test template 1', content: 'Test 1' },
                { title: 'Test template 2', content: 'Test 2' }
            ]
        });
    </script>
}