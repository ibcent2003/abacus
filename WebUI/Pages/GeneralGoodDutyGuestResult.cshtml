                                                    @{
                                                        var totalCIF = Model.userHscode.FOB + Model.userHscode.Freight + Model.userHscode.Insurance;

                                                    }
                                                    @totalCIF.ToString("#,##0.00")
                                                </td>
                                            </tr>

                                            <tr>
                                                <td style="background-color:lightgray">Exhange Rate</td>
                                                <td>
                                                    @Model.extraTax.TaxValue
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>

