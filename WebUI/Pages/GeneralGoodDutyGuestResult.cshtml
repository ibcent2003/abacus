                                                <tbody>

                                                    <tr class="warning">
                                                        <td>
                                                            @t.TaxName

                                                        </td>
                                                        @if (t.IsRate == true)
                                                        {
                                                            <td>@t.TaxValue%</td>
                                                        }
                                                        else
                                                        {
                                                            if (t == Model.userTariffExtraTaxes.Last())
                                                            {
                                                                <td>@Model.country.CurrencyCode @t.TaxValue.ToString("#,##0.00")</td>
                                                            }
                                                            else
                                                            {
                                                                <td>@Model.currency.Name @t.TaxValue.ToString("#,##0.00")</td>
                                                            }
                                                        }

                                                    </tr>


                                                </tbody>
                                            }
                                        </table>
                                        @if (Model.HasBechMark == true)
