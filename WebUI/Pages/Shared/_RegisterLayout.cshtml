    <meta name="description" content=" Classification Tool, HS Codes commodity classification tool, Product classification tool">
    <meta name="keywords" content="HSCode, product, commodity, classification, code">

    <link rel="shortcut icon" type="image/x-icon" href="assets/images/logo_sw.png">
    <title>eTrade Hub - Classification Tool</title>
    <link rel="stylesheet" href="~/assets/css/animate.css">
    <!-- Latest Bootstrap min CSS -->
    <link rel="stylesheet" href="~/assets/bootstrap/css/bootstrap.min.css">
    <!-- Google Font -->
